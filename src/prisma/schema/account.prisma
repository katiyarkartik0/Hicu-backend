model Account {
  id                  Int                   @id @default(autoincrement())
  name                String
  description         String?
  website             String?
  svgIcon             String?
  createdAt           DateTime              @default(now())
  updatedAt           DateTime              @updatedAt
  igUserId            String?               @unique
  igUsername          String?               @unique
  igFollowersCount    Int?
  igFollowingCount    Int?
  igMediaCount        Int?
  igName              String?
  igBiography         String?
  igProfilePictureUrl String?
  igAccountType       String?
  members             AccountMember[]
  configurations      Configurations[]
  IgCommentAutomation IgCommentAutomation[]
  IgDmAutomation      IgDmAutomation?
  Leads               Leads?
  Prospect            Prospect[]
  IgMedia             IgMedia[]
  IgComment           IgComment[]
  IgDm                IgDm[]
}
